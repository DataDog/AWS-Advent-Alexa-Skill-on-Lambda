{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalDecorators": true
    }
  },
  "settings": {},
  "rules": {
    "no-console": "off",
    "max-len": [
      "error",
      120,
      2
    ]
  }
}